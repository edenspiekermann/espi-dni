<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title><%= gettext "Default Title" %></title>
  <meta name="description" content="<%= gettext "Default Description" %>">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="apple-touch-icon" sizes="180x180" href="<%= static_path(@conn, "/apple-touch-icon.png") %>">
  <link rel="icon" type="image/png" href="<%= static_path(@conn, "/favicon-32x32.png") %>" sizes="32x32">
  <link rel="icon" type="image/png" href="<%= static_path(@conn, "/favicon-16x16.png") %>" sizes="16x16">
  <link rel="manifest" href="<%= static_path(@conn, "/manifest.json") %>">
  <link rel="mask-icon" href="<%= static_path(@conn, "/safari-pinned-tab.svg") %>" color="#5bbad5">
  <meta name="theme-color" content="#ffffff">

  <meta name="twitter:card" content="summary">
  <meta name="twitter:title" content="<%= gettext "Default Title" %>">
  <meta name="twitter:description" content="<%= gettext "Default Description" %>">
  <meta name="twitter:image" content="<%= static_path(@conn, "/images/post_logo.png") %>">

  <meta property="og:title" content="<%= gettext "Default Title" %>" />
  <meta property="og:type" content="website" />
  <meta property="og:url" content="<%= page_url(@conn, :index) %>" />
  <meta property="og:image" content="<%= static_path(@conn, "/images/post_logo.png") %>" />
  <meta property="og:description" content="<%= gettext "Default Description" %>" />

  <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/normalize/4.2.0/normalize.min.css">
  <link rel="stylesheet" href="<%= static_path(@conn, "/css/app.css") %>">
  <!--[if lt IE 9]>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body>
  <!--[if lt IE 8]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
  <![endif]-->

  <header class="clearfix site-header">
    <div class="header container">
      <img class="header-logo-espi" src="/images/edenspiekermann.png" alt="Edenspiekermann logo" />
      <img class="header-logo-dni" src="/images/dni_logogoogle_-01_720.png" alt="Google Digital News Initiative logo" />
    </div>
  </header>

  <div "alert">
    <p class="alert__info" role="alert"><%= get_flash(@conn, :info) %></p>
    <p class="alert__danger" role="alert"><%= get_flash(@conn, :error) %></p>
  </div>

  <main role="main">
    <%= render @view_module, @view_template, assigns %>
  </main>

  <script src="<%= static_path(@conn, "/js/app.js") %>"></script>
</body>
</html>
